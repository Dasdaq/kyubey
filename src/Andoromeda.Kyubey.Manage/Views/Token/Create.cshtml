@{
    ViewBag.Title = SR["Create Token"];
    ViewBag.Current = "token-create";
}

<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">@ViewBag.Title</h1>
</div>

<form class="col-lg-6 col-md-12" asp-action="Create" method="post">
    <div class="form-group"><label>@SR["Symbol"]</label><br /><input type="text" name="id" class="form-control" /></div>
    <div class="form-group"><label>@SR["Name"]</label><br /><input type="text" name="name" class="form-control" /></div>
    <div class="form-group"><input type="submit" value="@SR["Create"]" class="btn btn-primary" /></div>
</form>
