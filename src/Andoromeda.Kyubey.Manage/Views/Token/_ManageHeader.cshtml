@model Token

<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">@ViewBag.Title</h1>
</div>

<div class="container-fluid">
    <ul class="manage-header">
        <li class="manage-header-item @(ViewBag.Current == "token-manage-basic" ? "active" : "")"><a asp-action="Manage" asp-route-id="@Model.Id">@SR["Basic"]</a></li>
        <li class="manage-header-item @(ViewBag.Current == "token-manage-description" ? "active" : "")"><a asp-action="ManageDescription" asp-route-id="@Model.Id">@SR["Description"]</a></li>
        <li class="manage-header-item @(ViewBag.Current == "token-manage-icon" ? "active" : "")"><a asp-action="ManageIcon" asp-route-id="@Model.Id">@SR["Icon"]</a></li>
        <li class="manage-header-item @(ViewBag.Current == "token-manage-curve" ? "active" : "")"><a asp-action="ManageCurve" asp-route-id="@Model.Id">@SR["Curve"]</a></li>
        <li class="manage-header-item @(ViewBag.Current == "token-manage-exchange" ? "active" : "")"><a asp-action="ManageExchange" asp-route-id="@Model.Id">@SR["Exchange"]</a></li>
        @*<li class="manage-header-item @(ViewBag.Current == "token-manage-pricing" ? "active" : "")"><a asp-action="ManagePricing" asp-route-id="@Model.Id">@SR["Pricing"]</a></li>*@
    </ul>
</div>