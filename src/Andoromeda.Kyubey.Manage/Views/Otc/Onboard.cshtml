@model Token
@{
    ViewBag.Title = Model.Id + " - " + SR["Onboard_OTC"];
}

<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">@ViewBag.Title</h1>
</div>

<form class="col-lg-6 col-md-12" asp-action="Onboard" method="post">
    <div class="form-group"><label>@SR["Symbol"]</label><br /><input type="text" name="id" class="form-control" value="@Model.Id" disabled /></div>
    <div class="form-group"><input type="submit" value="@SR["Onboard_OTC"]" class="btn btn-primary" /></div>
</form>
